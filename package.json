{
  "name": "json-schema-codec-generator",
  "version": "0.7.2",
  "description": "Generate typed codec validator code from collections of json-schema",
  "keywords": [
    "json-schema",
    "codegen",
    "cli",
    "typescript",
    "typed"
  ],
  "author": "Geoffrey Goodman",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "json-schema-codec-generator": "bin/json-schema-codec-generator"
  },
  "browser": {},
  "exports": {
    ".": {
      "node": {
        "require": "./dist/index.js",
        "import": "./dist/index.esm.js"
      },
      "default": "./dist/index.js"
    },
    "./dist/": "./dist/"
  },
  "files": [
    "dist/**/!(tsconfig.tsbuildinfo)"
  ],
  "dependencies": {
    "@ggoodman/typed-validator": "^0.2.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.0.2",
    "esbuild-wasm": "^0.11.6",
    "fast-glob": "^3.2.5",
    "json-schema-to-dts": "^1.5.0",
    "prettier": "^2.2.1",
    "resolve": "^1.20.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.18.13",
    "@types/resolve": "^1.20.0",
    "gh-release": "^6.0.4",
    "jest": "^27.5.1",
    "kacl": "^1.1.1",
    "rollup": "^2.41.4",
    "rollup-plugin-ts": "^2.0.7",
    "ts-jest": "^27.1.4"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c rollup.config.js",
    "lint": "prettier --check src/**/*",
    "prepack": "npm run build",
    "test": "jest --verbose",
    "posttest": "kacl lint",
    "preversion": "kacl prerelease",
    "version": "kacl release && git add CHANGELOG.md",
    "postversion": "git push && gh-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggoodman/json-schema-codec-generator.git"
  },
  "bugs": {
    "url": "https://github.com/ggoodman/json-schema-codec-generator/issues"
  },
  "homepage": "https://github.com/ggoodman/json-schema-codec-generator#readme",
  "engines": {
    "node": ">=10.16.0"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 100,
    "singleQuote": true
  },
  "volta": {
    "node": "16.14.2",
    "npm": "8.7.0"
  }
}
